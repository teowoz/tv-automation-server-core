{
  "name": "automation-core",
  "version": "1.5.0",
  "private": true,
  "engines": {
    "node": "8.x"
  },
  "scripts": {
    "start": "meteor run",
    "debug": "meteor run --inspect",
    "postinstall": "meteor npm run prepareForTest",
    "prepareForTest": "node ../scripts/fixTestFibers.js",
    "unit": "jest",
    "unitci": "jest --maxWorkers 2 --coverage",
    "unitcov": "jest --coverage",
    "test": "meteor npm run unit",
    "watch": "jest --watch",
    "update-snapshots": "jest --updateSnapshot",
    "ci_tmp": "meteor npm run lint && meteor npm run cov",
    "ci": "meteor npm run unitci && meteor npm run send-coverage",
    "cov-open": "open-cli coverage/lcov-report/index.html",
    "cov": "meteor npm run unitcov && meteor npm run cov-open",
    "send-coverage": "codecov",
    "test-i": "npm run integration-test",
    "license-validate": "node-license-validator -p -d --allow-licenses MIT BSD BSD-3-Clause ISC Apache Public-Domain 'Public Domain' Unlicense WTFPL CC-BY-4.0 --allow-packages cycle @fortawesome/fontawesome-common-types",
    "lint": "tslint --project tsconfig.test.json --config tslint.json",
    "lintfix": "tslint --project tsconfig.test.json --config tslint.json --fix",
    "i18n-extract-pot": "i18next-extract-gettext --files=\"{./client/ui/**/*.+(ts|tsx),./lib/mediaObjects.ts}\" --output=i18n/template.pot",
    "i18n-compile-json": "npm run i18n-compile-json-nb & npm run i18n-compile-json-nn & npm run i18n-compile-json-sv",
    "i18n-compile-json-nb": "i18next-conv -l nb -s i18n/nb.po -t public/locales/nb/translations.json --skipUntranslated",
    "i18n-compile-json-nn": "i18next-conv -l nn -s i18n/nn.po -t public/locales/nn/translations.json --skipUntranslated",
    "i18n-compile-json-sv": "i18next-conv -l sv -s i18n/sv.po -t public/locales/sv/translations.json --skipUntranslated",
    "release": "standard-version",
    "prepareChangelog": "standard-version --prerelease",
    "validate-dependencies": "npm audit && npm run license-validate",
    "deprecated-unit-test": "TEST_WATCH=1 meteor test --driver-package meteortesting:mocha",
    "deprecated-integration-test": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "deprecated-test-once": "meteor test --once --driver-package meteortesting:mocha"
  },
  "dependencies": {
    "@babel/runtime": "7.0.0-beta.48",
    "@fortawesome/fontawesome": "^1.1.7",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "0.0.19",
    "@nrk/core-icons": "^3.2.0",
    "@slack/client": "^4.6.0",
    "amqplib": "^0.5.3",
    "body-parser": "^1.18.3",
    "caller-module": "^1.0.1",
    "classnames": "^2.2.5",
    "core-js": "^2.5.4",
    "element-resize-event": "^3.0.3",
    "fast-clone": "^1.5.3",
    "html-entities": "^1.2.1",
    "i18next": "^11.2.3",
    "i18next-browser-languagedetector": "^2.2.0",
    "i18next-xhr-backend": "^1.5.1",
    "indexof": "0.0.1",
    "lottie-web": "^5.1.10",
    "meteor-node-stubs": "^0.3.3",
    "moment": "^2.19.1",
    "mos-connection": "^0.7.0",
    "node-expat": "^2.3.18",
    "ntp-client": "^0.5.3",
    "object-path": "^0.11.4",
    "prop-types": "^15.6.1",
    "query-string": "^6.4.2",
    "rc-tooltip": "^3.7.3",
    "react": "^16.3.0",
    "react-bootstrap": "^0.32.1",
    "react-circular-progressbar": "^1.0.0",
    "react-contextmenu": "^2.9.2",
    "react-datepicker": "^1.6.0",
    "react-dom": "^16.3.0",
    "react-escape": "0.0.8",
    "react-hotkeys": "^0.10.0",
    "react-i18next": "^7.13.0",
    "react-intersection-observer": "^8.23.0",
    "react-lottie": "^1.1.0",
    "react-moment": "^0.7.0",
    "react-router-dom": "^4.2.2",
    "react-timer-hoc": "^2.3.0",
    "safer-eval": "^1.2.3",
    "semver": "^6.0.0",
    "soap": "^0.24.0",
    "superfly-timeline": "^7.3.1",
    "timecode": "0.0.4",
    "tv-automation-sofie-blueprints-integration": "1.8.0-nightly-20200211-122425-a12f6c7.0",
    "underscore": "^1.8.3",
    "velocity-animate": "^1.5.1",
    "velocity-react": "^1.3.3",
    "winston": "^2.4.2",
    "xml2json": "^0.11.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.9",
    "@types/body-parser": "^1.17.0",
    "@types/classnames": "2.2.3",
    "@types/css-font-loading-module": "0.0.1",
    "@types/fibers": "0.0.30",
    "@types/jest": "^24.0.19",
    "@types/meteor": "1.4.21",
    "@types/mousetrap": "^1.6.0",
    "@types/node": "^8.10.4",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.3.2",
    "@types/react-circular-progressbar": "^1.0.1",
    "@types/react-dom": "^16.0.4",
    "@types/react-i18next": "^7.8.3",
    "@types/react-router-dom": "^4.2.6",
    "@types/request": "^2.48.1",
    "@types/semver": "^6.0.0",
    "@types/sinon": "^7.0.12",
    "@types/winston": "^2.3.9",
    "@types/xml2json": "^0.11.0",
    "codecov": "^3.5.0",
    "ejson": "^2.2.0",
    "fibers": "^3.1.1",
    "i18next-conv": "^8.0.1",
    "i18next-extract-gettext": "^3.2.0",
    "jest": "^24.9.0",
    "meteor-promise": "0.8.7",
    "mock-http": "^1.0.2",
    "node-license-validator": "^1.3.0",
    "open-cli": "^5.0.0",
    "sinon": "^7.3.2",
    "standard-version": "^6.0.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.8.0",
    "typescript": "2.8.3",
    "typescript-tslint-plugin": "^0.4.0"
  }
}
